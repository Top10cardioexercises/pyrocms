{
    "name": "pyrocms/pyrocms",
    "description": "Build better Laravel websites faster with Pyro.",
    "type": "project",
    "keywords": [
        "cms",
        "pyro",
        "pyrocms",
        "php cms",
        "laravel cms",
        "flatfile cms",
        "flat file cms"
    ],
    "authors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {
        "streams/core": "~2.0.0",
        "streams/ui": "~1.0.0"
    },
    "require-dev": {
        "streams/cli": "~1.0.0",
        "spatie/laravel-web-tinker": "^1.8"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
