{
    "name": "pyrocms/pyrocms",
    "description": "Build better Laravel websites faster with Pyro.",
    "type": "project",
    "keywords": [
        "flatfile cms",
        "laravel cms"
    ],
    "authors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {
        "streams/core": "~2.0.0",
        "streams/ui": "~1.0.0",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "owenvoke/blade-fontawesome": "^2.0",
        "blade-ui-kit/blade-icons": "^1.3"
    },
    "require-dev": {
        "streams/cli": "~1.0.0",
        "barryvdh/laravel-debugbar": "^3.3",
        "spatie/laravel-web-tinker": "^1.8"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/streams-core",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/streams-ui",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-stable": false,
        "minimum-stability": "dev",
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
